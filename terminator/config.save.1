[global_config]
  borderless = True
  tab_position = bottom
  title_font = YaHei Monaco Hybird 19
  title_hide_sizetext = True
  title_use_system_font = False
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.0
    background_type = transparent
    cursor_color = "#aaaaaa"
    font = YaHei Monaco Hybird Bold 10
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
